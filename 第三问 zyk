import pandas as pd
import numpy as np
from sklearn.cluster import AgglomerativeClustering
from sklearn.ensemble import RandomForestRegressor
import matplotlib.pyplot as plt
import seaborn as sns
from scipy.cluster.hierarchy import dendrogram, linkage
import matplotlib
import re

# è®¾ç½®ä¸­æ–‡å­—ä½“
matplotlib.rcParams['font.sans-serif'] = ['SimHei', 'DejaVu Sans']
matplotlib.rcParams['axes.unicode_minus'] = False

# è¯»å–æ•°æ®
df_male = pd.read_csv('cleaned_dataæœ€ç»ˆ(1).csv')


# è½¬æ¢å­•å‘¨æ ¼å¼ä¸ºæ•°å€¼
def convert_week_to_numeric(week_str):
    if pd.isna(week_str):
        return np.nan
    if isinstance(week_str, (int, float)):
        return week_str

    week_str = str(week_str).lower().replace('å‘¨', '').replace('w', '')
    match = re.search(r'(\d+)(?:\+(\d+))?', week_str)
    if match:
        weeks = int(match.group(1))
        days = int(match.group(2)) if match.group(2) else 0
        return weeks + days / 7
    return np.nan


df_male['æ£€æµ‹å­•å‘¨æ•°å€¼'] = df_male['æ£€æµ‹å­•å‘¨'].apply(convert_week_to_numeric)

# å…³é”®ä¿®æ­£ï¼šå¯¹æ¯ä¸ªå­•å¦‡æ‰¾åˆ°æœ€æ—©è¾¾æ ‡æ—¶é—´
print("å¤„ç†æ¯ä¸ªå­•å¦‡çš„æœ€æ—©è¾¾æ ‡æ—¶é—´...")

# æ ‡è®°è¾¾æ ‡æ ·æœ¬
df_male['è¾¾æ ‡'] = df_male['YæŸ“è‰²ä½“æµ“åº¦'] >= 0.04

# å¯¹æ¯ä¸ªå­•å¦‡ï¼Œæ‰¾åˆ°æœ€æ—©è¾¾æ ‡çš„æ£€æµ‹æ—¶é—´
earliest_pass_df = df_male[df_male['è¾¾æ ‡']].groupby('å­•å¦‡ä»£ç ').agg({
    'æ£€æµ‹å­•å‘¨æ•°å€¼': 'min',
    'YæŸ“è‰²ä½“æµ“åº¦': 'first',
    'å­•å¦‡BMI': 'first',
    'å¹´é¾„': 'first',
    'èº«é«˜': 'first',
    'ä½“é‡': 'first'
}).reset_index()

earliest_pass_df.rename(columns={'æ£€æµ‹å­•å‘¨æ•°å€¼': 'æœ€æ—©è¾¾æ ‡å­•å‘¨'}, inplace=True)

print(f"æ€»å­•å¦‡æ•°: {df_male['å­•å¦‡ä»£ç '].nunique()}")
print(f"æœ‰è¾¾æ ‡è®°å½•çš„å­•å¦‡æ•°: {len(earliest_pass_df)}")
print(f"è¾¾æ ‡å­•å¦‡æ¯”ä¾‹: {len(earliest_pass_df) / df_male['å­•å¦‡ä»£ç '].nunique() * 100:.1f}%")

# åˆå¹¶å›åŸå§‹æ•°æ®ï¼Œä¸ºæ¯ä¸ªå­•å¦‡æ·»åŠ æœ€æ—©è¾¾æ ‡æ—¶é—´
df_male = df_male.merge(earliest_pass_df[['å­•å¦‡ä»£ç ', 'æœ€æ—©è¾¾æ ‡å­•å‘¨']], on='å­•å¦‡ä»£ç ', how='left')

# å¯¹äºæ²¡æœ‰è¾¾æ ‡è®°å½•çš„å­•å¦‡ï¼Œè®¾ç½®æœ€æ—©è¾¾æ ‡å­•å‘¨ä¸ºNaN
df_male.loc[df_male['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna(), 'æœ€æ—©è¾¾æ ‡å­•å‘¨'] = np.nan

# 1. ç‰¹å¾é€‰æ‹©
feature_rename = {
    'å¹´é¾„': 'å¹´é¾„',
    'èº«é«˜': 'èº«é«˜',
    'ä½“é‡': 'ä½“é‡',
    'å­•å¦‡BMI': 'BMI',
    'GCå«é‡': 'æ€»GCå«é‡',
    '13å·æŸ“è‰²ä½“çš„GCå«é‡': 'GC13',
    '18å·æŸ“è‰²ä½“çš„GCå«é‡': 'GC18',
    '21å·æŸ“è‰²ä½“çš„GCå«é‡': 'GC21'
}

features = list(feature_rename.keys())

# ä½¿ç”¨æ¯ä¸ªå­•å¦‡çš„ç¬¬ä¸€æ¡è®°å½•è¿›è¡Œåˆ†æï¼ˆé¿å…é‡å¤ï¼‰
unique_patients = df_male.drop_duplicates('å­•å¦‡ä»£ç ').copy()

# æ£€æŸ¥å¹¶å¤„ç†æç«¯BMIå€¼ï¼ˆä¸´åºŠåˆç†æ€§æ ¡éªŒï¼‰
bmi_threshold = 40  # ä¸´åºŠå®šä¹‰çš„é‡åº¦è‚¥èƒ–æ ‡å‡†
extreme_bmi_count = sum(unique_patients['å­•å¦‡BMI'] > bmi_threshold)
print(
    f"\nä¸´åºŠå¼‚å¸¸å€¼æç¤ºï¼šBMI>40çš„é‡åº¦è‚¥èƒ–å­•å¦‡å…±{extreme_bmi_count}äººï¼Œå æ€»æ ·æœ¬{extreme_bmi_count / len(unique_patients) * 100:.1f}%")

# è®¡ç®—ç›¸å…³ç³»æ•°
plot_data = unique_patients[features + ['YæŸ“è‰²ä½“æµ“åº¦']].copy()
plot_data.rename(columns=feature_rename, inplace=True)

corr = plot_data.corr()
plt.figure(figsize=(12, 10))
sns.heatmap(corr, annot=True, cmap='coolwarm', fmt='.2f',
            annot_kws={'size': 10}, cbar_kws={'shrink': 0.8})
plt.title("ç‰¹å¾ä¸YæŸ“è‰²ä½“æµ“åº¦çš„ç›¸å…³ç³»æ•°çƒ­åŠ›å›¾", fontsize=14, pad=20)
plt.tight_layout()
plt.savefig('Q3-ç›¸å…³ç³»æ•°çƒ­åŠ›å›¾.png', dpi=300, bbox_inches='tight')
plt.close()

# éšæœºæ£®æ—ç‰¹å¾é‡è¦æ€§
X = unique_patients[features].fillna(0)
y = unique_patients['YæŸ“è‰²ä½“æµ“åº¦'].fillna(0)
model = RandomForestRegressor(random_state=42)
model.fit(X, y)

importance_df = pd.DataFrame({
    'ç‰¹å¾': [feature_rename.get(f, f) for f in features],
    'é‡è¦æ€§': model.feature_importances_
}).sort_values('é‡è¦æ€§', ascending=False)

print("\néšæœºæ£®æ—ç‰¹å¾é‡è¦æ€§:")
print(importance_df.to_string(index=False))
print()

# è§£é‡Šç‰¹å¾é‡è¦æ€§çš„ä¸´åºŠæ„ä¹‰
print("ç‰¹å¾é‡è¦æ€§ä¸´åºŠè§£é‡Š:")
print("- ä½“é‡å’ŒBMIæ˜¯å½±å“YæŸ“è‰²ä½“æµ“åº¦çš„ä¸»è¦å› ç´ ï¼Œæç¤ºè‚¥èƒ–å¯èƒ½é™ä½æ£€æµ‹çµæ•åº¦")
print("- 13ã€18ã€21å·æŸ“è‰²ä½“GCå«é‡ä¸æ£€æµ‹å‡†ç¡®æ€§ç›¸å…³ï¼ŒGCå«é‡å¼‚å¸¸å¯èƒ½å¢åŠ æ£€æµ‹éš¾åº¦")
print()

# 3. å±‚æ¬¡èšç±»BMIåˆ†ç»„ï¼ˆåŸºäºå”¯ä¸€å­•å¦‡ï¼‰
bmi_data = unique_patients[['å­•å¦‡BMI']].dropna().values

plt.figure(figsize=(12, 6))
Z = linkage(bmi_data, method='ward')
dendrogram(Z, truncate_mode='level', p=5)
plt.title('BMIå±‚æ¬¡èšç±»æ ‘çŠ¶å›¾')
plt.xlabel('æ ·æœ¬ç´¢å¼•')
plt.ylabel('è·ç¦»')
plt.axhline(y=15, color='r', linestyle='--')
plt.savefig('Q3-BMIå±‚æ¬¡èšç±»æ ‘çŠ¶å›¾.png', dpi=300, bbox_inches='tight')
plt.close()

n_clusters = 4
hierarchical = AgglomerativeClustering(n_clusters=n_clusters, metric='euclidean', linkage='ward')
unique_patients['BMIåˆ†ç»„'] = hierarchical.fit_predict(bmi_data)

# æŒ‰å¹³å‡BMIæ’åºåˆ†ç»„ï¼Œç¡®ä¿åˆ†ç»„æŒ‰BMIå€¼é€’å¢
group_order = unique_patients.groupby('BMIåˆ†ç»„')['å­•å¦‡BMI'].mean().sort_values().index
group_mapping = {old: new for new, old in enumerate(group_order)}
unique_patients['BMIåˆ†ç»„'] = unique_patients['BMIåˆ†ç»„'].map(group_mapping)

# å®šä¹‰ä¸´åºŠåˆ†ç»„åç§°
clinical_group_names = {
    0: "æ­£å¸¸/è¶…é‡",
    1: "è½»åº¦è‚¥èƒ–",
    2: "ä¸­åº¦è‚¥èƒ–",
    3: "é‡åº¦è‚¥èƒ–"
}
unique_patients['BMIä¸´åºŠåˆ†ç»„'] = unique_patients['BMIåˆ†ç»„'].map(clinical_group_names)

# å°†åˆ†ç»„ç»“æœæ˜ å°„å›åŸå§‹æ•°æ®
bmi_group_mapping = unique_patients[['å­•å¦‡ä»£ç ', 'BMIåˆ†ç»„', 'BMIä¸´åºŠåˆ†ç»„']]
df_male = df_male.merge(bmi_group_mapping, on='å­•å¦‡ä»£ç ', how='left')

# è¾“å‡ºæ¯ä¸ªåˆ†ç»„çš„BMIåŒºé—´
group_stats = unique_patients.groupby(['BMIåˆ†ç»„', 'BMIä¸´åºŠåˆ†ç»„'])['å­•å¦‡BMI'].agg(
    ['min', 'max', 'mean', 'count']).reset_index()
print("å„BMIåˆ†ç»„çš„ç»Ÿè®¡ä¿¡æ¯:")
print(group_stats.to_string(index=False))
print()


# 4. æ”¹è¿›çš„é£é™©å‡½æ•°ï¼ˆç»“åˆå­•å‘¨å’ŒBMIï¼Œç¬¦åˆä¸´åºŠé€»è¾‘ï¼‰
def risk(week, bmi):
    """
    åŸºäºä¸´åºŠå®è·µçš„é£é™©è¯„ä¼°å‡½æ•°ï¼š
    - è€ƒè™‘NIPTæœ€ä½³æ£€æµ‹çª—å£ï¼ˆ12-22å‘¨ï¼‰
    - ç»“åˆBMIå¯¹æ£€æµ‹éš¾åº¦çš„å½±å“
    - é£é™©ç­‰çº§ï¼š0=ä½é£é™©ï¼Œ1=ä¸­é£é™©ï¼Œ2=é«˜é£é™©ï¼Œ3=æé«˜é£é™©
    """
    if pd.isna(week):
        return 3  # æœªè¾¾æ ‡â†’æé«˜é£é™©ï¼ˆéœ€è¿›ä¸€æ­¥æ£€æŸ¥ï¼‰

    # ä¸´åºŠæœ€ä½³æ£€æµ‹çª—å£ï¼š12-22å‘¨
    if 12 <= week <= 22:
        if bmi < 30:
            return 0  # æ­£å¸¸BMI+æœ€ä½³çª—å£â†’ä½é£é™©
        elif 30 <= bmi < 38:
            return 1  # é«˜BMI+æœ€ä½³çª—å£â†’ä¸­é£é™©
        else:
            return 2  # é‡åº¦è‚¥èƒ–+æœ€ä½³çª—å£â†’é«˜é£é™©
    # å¯æ¥å—æ£€æµ‹çª—å£ï¼š22-27å‘¨
    elif 22 < week <= 27:
        return 2  # å¯æ¥å—çª—å£â†’é«˜é£é™©ï¼ˆæ— è®ºBMIï¼Œéœ€åŠ å¿«å†³ç­–ï¼‰
    # éç†æƒ³çª—å£ï¼š<12å‘¨ï¼ˆå¤±è´¥ç‡é«˜ï¼‰æˆ–>27å‘¨ï¼ˆå»¶è¯¯å¹²é¢„ï¼‰
    else:
        return 3  # æé«˜é£é™©


# è®¡ç®—æ¯ä¸ªå­•å¦‡çš„é£é™©å€¼
unique_patients['é£é™©å€¼'] = unique_patients.apply(
    lambda row: risk(row['æœ€æ—©è¾¾æ ‡å­•å‘¨'], row['å­•å¦‡BMI']), axis=1)

# 5. æœ€ä½³æ—¶ç‚¹é€‰æ‹©
groups = unique_patients.groupby(['BMIåˆ†ç»„', 'BMIä¸´åºŠåˆ†ç»„'])
best_weeks = {}

print("å„BMIåˆ†ç»„çš„æœ€ä½³NIPTæ—¶ç‚¹:")
print("-" * 100)
for (group_id, group_name), group_df in groups:
    if len(group_df) > 0:
        # è®¡ç®—è¯¥ç»„ä¸­è¾¾æ ‡å­•å¦‡çš„æœ€æ—©è¾¾æ ‡å­•å‘¨åˆ†å¸ƒ
        passed_patients = group_df[~group_df['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna()]

        if len(passed_patients) > 0:
            # æ‰¾åˆ°èƒ½å¤Ÿè¦†ç›–ä¸€å®šæ¯”ä¾‹å­•å¦‡çš„å­•å‘¨
            sorted_weeks = passed_patients['æœ€æ—©è¾¾æ ‡å­•å‘¨'].sort_values()
            # é€‰æ‹©ç¬¬75ç™¾åˆ†ä½çš„å­•å‘¨ä½œä¸ºæ¨èæ—¶ç‚¹ï¼ˆè¦†ç›–75%çš„è¾¾æ ‡å­•å¦‡ï¼‰
            recommended_week = sorted_weeks.quantile(0.75)

            # è®¡ç®—è¯¥ç»„çš„BMIå‡å€¼ç”¨äºé£é™©è¯„ä¼°
            avg_bmi = group_df['å­•å¦‡BMI'].mean()
            # è®¡ç®—åœ¨è¯¥å­•å‘¨æ£€æµ‹çš„é£é™©
            risk_at_week = risk(recommended_week, avg_bmi)

            best_weeks[group_id] = (recommended_week, risk_at_week, group_name, avg_bmi)

            bmi_min = group_df['å­•å¦‡BMI'].min()
            bmi_max = group_df['å­•å¦‡BMI'].max()

            print(f"åˆ†ç»„ {group_id + 1} ({group_name}, BMIèŒƒå›´: {bmi_min:.1f}-{bmi_max:.1f}):")
            print(f"  æ¨èæ£€æµ‹å­•å‘¨: {recommended_week:.2f} å‘¨")
            print(f"  åœ¨è¯¥å­•å‘¨æ£€æµ‹çš„é£é™©ç­‰çº§: {risk_at_week}")
            print(
                f"  é£é™©è§£é‡Š: {'ä½é£é™©' if risk_at_week == 0 else 'ä¸­é£é™©' if risk_at_week == 1 else 'é«˜é£é™©' if risk_at_week == 2 else 'æé«˜é£é™©'}")

            # ç»Ÿè®¡ä¿¡æ¯
            total_patients = len(group_df)
            passed_patients_count = len(passed_patients)
            pass_rate = (passed_patients_count / total_patients) * 100
            fail_rate = 100 - pass_rate

            print(f"  å­•å¦‡æ€»æ•°: {total_patients}")
            print(f"  è¾¾æ ‡å­•å¦‡æ•°: {passed_patients_count}")
            print(f"  è¾¾æ ‡ç‡: {pass_rate:.1f}%")
            print(f"  é¢„ä¼°å¤±è´¥ç‡: {fail_rate:.1f}%")
            print(f"  å¹³å‡æœ€æ—©è¾¾æ ‡å­•å‘¨: {passed_patients['æœ€æ—©è¾¾æ ‡å­•å‘¨'].mean():.2f}å‘¨")
            print("-" * 100)
        else:
            print(f"åˆ†ç»„ {group_id + 1} ({group_name}): æ²¡æœ‰å­•å¦‡è¾¾åˆ°4%çš„YæŸ“è‰²ä½“æµ“åº¦")
            print("-" * 100)

# å¯è§†åŒ–ç»“æœ - æ¯ä¸ªå›¾å•ç‹¬ä¿å­˜

# å›¾1: å„åˆ†ç»„æœ€æ—©è¾¾æ ‡å­•å‘¨åˆ†å¸ƒï¼ˆå¢åŠ å››åˆ†ä½èŒƒå›´ï¼‰
plt.figure(figsize=(10, 6))
for group_id in range(n_clusters):
    group_data = unique_patients[unique_patients['BMIåˆ†ç»„'] == group_id]
    passed_data = group_data[~group_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna()]
    group_name = group_data['BMIä¸´åºŠåˆ†ç»„'].iloc[0]

    if len(passed_data) > 0:
        # ç»˜åˆ¶æ•£ç‚¹
        plt.scatter([group_id + 1] * len(passed_data), passed_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'],
                    alpha=0.6, label=f'{group_name}')

        # æ ‡æ³¨ä¸­ä½æ•°å’Œå››åˆ†ä½èŒƒå›´
        median_week = passed_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'].median()
        q1 = passed_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'].quantile(0.25)
        q3 = passed_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'].quantile(0.75)

        plt.plot(group_id + 1, median_week, 'ro', markersize=8)
        # ç»˜åˆ¶å››åˆ†ä½çº¿
        plt.plot([group_id + 0.9, group_id + 1.1], [q1, q1], 'r-', linewidth=2)
        plt.plot([group_id + 0.9, group_id + 1.1], [q3, q3], 'r-', linewidth=2)
        plt.plot([group_id + 1, group_id + 1], [q1, q3], 'r-', linewidth=2)

# æ ‡æ³¨ä¸´åºŠæœ€ä½³æ£€æµ‹çª—å£
plt.axhspan(12, 22, color='green', alpha=0.2, label='æœ€ä½³æ£€æµ‹çª—å£(12-22å‘¨)')
plt.axhspan(22, 27, color='yellow', alpha=0.2, label='å¯æ¥å—çª—å£(22-27å‘¨)')
plt.axhline(y=12, color='g', linestyle='--', alpha=0.7)
plt.axhline(y=22, color='y', linestyle='--', alpha=0.7)
plt.axhline(y=27, color='orange', linestyle='--', alpha=0.7)

plt.xlabel('BMIåˆ†ç»„')
plt.ylabel('æœ€æ—©è¾¾æ ‡å­•å‘¨')
plt.title('å„åˆ†ç»„å­•å¦‡çš„æœ€æ—©è¾¾æ ‡å­•å‘¨åˆ†å¸ƒï¼ˆå«å››åˆ†ä½èŒƒå›´ï¼‰')
plt.xticks(range(1, n_clusters + 1), [f'åˆ†ç»„{i}' for i in range(1, n_clusters + 1)])
plt.legend()
plt.grid(True, alpha=0.3)
plt.tight_layout()
plt.savefig('Q3-å„åˆ†ç»„æœ€æ—©è¾¾æ ‡å­•å‘¨åˆ†å¸ƒ.png', dpi=300, bbox_inches='tight')
plt.close()

# å›¾2: å„åˆ†ç»„è¾¾æ ‡ç‡
plt.figure(figsize=(10, 6))
pass_rates = []
group_labels = []
for (group_id, group_name), group_df in groups:
    pass_rate = (len(group_df[~group_df['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna()]) / len(group_df)) * 100
    pass_rates.append(pass_rate)
    group_labels.append(f'{group_name}\n(åˆ†ç»„{group_id + 1})')

plt.bar(range(1, n_clusters + 1), pass_rates, color='skyblue')
plt.xlabel('BMIä¸´åºŠåˆ†ç»„')
plt.ylabel('è¾¾æ ‡ç‡ (%)')
plt.title('å„åˆ†ç»„çš„å­•å¦‡è¾¾æ ‡ç‡')
plt.xticks(range(1, n_clusters + 1), group_labels, rotation=30)
for i, v in enumerate(pass_rates):
    plt.text(i + 1, v + 1, f'{v:.1f}%', ha='center')
plt.grid(True, alpha=0.3)
plt.tight_layout()
plt.savefig('Q3-å„åˆ†ç»„è¾¾æ ‡ç‡.png', dpi=300, bbox_inches='tight')
plt.close()

# å›¾3: é£é™©åˆ†å¸ƒï¼ˆä½¿ç”¨ç™¾åˆ†æ¯”ï¼‰
plt.figure(figsize=(10, 6))
risk_counts = unique_patients.groupby(['BMIåˆ†ç»„', 'BMIä¸´åºŠåˆ†ç»„', 'é£é™©å€¼']).size().unstack(fill_value=0)
# è½¬æ¢ä¸ºç™¾åˆ†æ¯”
risk_percent = risk_counts.div(risk_counts.sum(axis=1), axis=0) * 100
risk_percent.plot(kind='bar', stacked=True, colormap='RdYlGn_r', figsize=(10, 6))
plt.xlabel('BMIåˆ†ç»„ (ä¸´åºŠåˆ†ç»„)')
plt.ylabel('å­•å¦‡æ¯”ä¾‹ (%)')
plt.title('å„åˆ†ç»„çš„é£é™©åˆ†å¸ƒï¼ˆç™¾åˆ†æ¯”ï¼‰')
plt.xticks(rotation=30)
plt.legend(['ä½é£é™©(0)', 'ä¸­é£é™©(1)', 'é«˜é£é™©(2)', 'æé«˜é£é™©(3)'])
plt.grid(True, alpha=0.3)
plt.tight_layout()
plt.savefig('Q3-é£é™©åˆ†å¸ƒ.png', dpi=300, bbox_inches='tight')
plt.close()

# å›¾4: BMIä¸æœ€æ—©è¾¾æ ‡å­•å‘¨çš„å…³ç³»
plt.figure(figsize=(10, 6))
passed_patients = unique_patients[~unique_patients['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna()]
scatter = plt.scatter(passed_patients['å­•å¦‡BMI'], passed_patients['æœ€æ—©è¾¾æ ‡å­•å‘¨'],
                      c=passed_patients['BMIåˆ†ç»„'], cmap='viridis', alpha=0.6)
plt.colorbar(scatter, label='BMIåˆ†ç»„')

# æ ‡æ³¨ä¸´åºŠæœ€ä½³æ£€æµ‹çª—å£
plt.axhspan(12, 22, color='green', alpha=0.2, label='æœ€ä½³æ£€æµ‹çª—å£(12-22å‘¨)')
plt.axhspan(22, 27, color='yellow', alpha=0.2, label='å¯æ¥å—çª—å£(22-27å‘¨)')

plt.xlabel('å­•å¦‡BMI')
plt.ylabel('æœ€æ—©è¾¾æ ‡å­•å‘¨')
plt.title('BMIä¸æœ€æ—©è¾¾æ ‡å­•å‘¨çš„å…³ç³»')
plt.grid(True, alpha=0.3)
plt.legend()
plt.tight_layout()
plt.savefig('Q3-BMIä¸è¾¾æ ‡å­•å‘¨å…³ç³».png', dpi=300, bbox_inches='tight')
plt.close()

print("\n æœ€ç»ˆNIPTæ—¶ç‚¹å»ºè®®ï¼ˆåŸºäºå­•å¦‡çº§åˆ«åˆ†æï¼‰:")
print("=" * 100)
for group_id in range(n_clusters):
    if group_id in best_weeks:
        recommended_week, risk_level, group_name, avg_bmi = best_weeks[group_id]
        group_data = unique_patients[unique_patients['BMIåˆ†ç»„'] == group_id]
        bmi_min = group_data['å­•å¦‡BMI'].min()
        bmi_max = group_data['å­•å¦‡BMI'].max()
        total_patients = len(group_data)
        passed_patients_count = len(group_data[~group_data['æœ€æ—©è¾¾æ ‡å­•å‘¨'].isna()])
        fail_rate = 100 - (passed_patients_count / total_patients) * 100

        print(f"{group_name} (BMI: {bmi_min:.1f}-{bmi_max:.1f}):")
        print(f"  æ¨èæ£€æµ‹æ—¶é—´: å­•{recommended_week:.1f}å‘¨")
        print(
            f"    é¢„æœŸé£é™©ç­‰çº§: {risk_level} ({'ä½é£é™©' if risk_level == 0 else 'ä¸­é£é™©' if risk_level == 1 else 'é«˜é£é™©' if risk_level == 2 else 'æé«˜é£é™©'})")
        print(f"  è¦†ç›–æ¯”ä¾‹: çº¦75%çš„è¾¾æ ‡å­•å¦‡åœ¨æ­¤å­•å‘¨å‰å·²è¾¾æ ‡")
        print(f"  é¢„ä¼°å¤±è´¥ç‡: {fail_rate:.1f}%ï¼Œå»ºè®®æå‰å‘ŠçŸ¥å­•å¦‡å¯èƒ½éœ€è¦å¤æµ‹")

        # é’ˆå¯¹ä¸åŒé£é™©ç­‰çº§çš„ä¸´åºŠå»ºè®®
        if risk_level == 0:
            print(f"  ğŸ’¡ ä¸´åºŠå»ºè®®: å¸¸è§„æ£€æµ‹æµç¨‹å³å¯ï¼Œæ— éœ€ç‰¹æ®Šå‡†å¤‡")
        elif risk_level == 1:
            print(f"  ğŸ’¡ ä¸´åºŠå»ºè®®: å»ºè®®ç©ºè…¹æ£€æµ‹ä»¥æé«˜å‡†ç¡®æ€§ï¼Œæ£€æµ‹å48å°æ—¶å†…å…³æ³¨ç»“æœ")
        elif risk_level == 2:
            print(f"  ğŸ’¡ ä¸´åºŠå»ºè®®: æ£€æµ‹å‰é¿å…é«˜è„‚é¥®é£Ÿï¼Œæ£€æµ‹å24å°æ—¶å†…å¤æŸ¥ï¼Œå¿…è¦æ—¶è€ƒè™‘å…¶ä»–æ£€æµ‹æ–¹å¼")
        else:
            print(f"  ğŸ’¡ ä¸´åºŠå»ºè®®: å¼ºçƒˆå»ºè®®é‡‡ç”¨å…¶ä»–è¾…åŠ©æ£€æµ‹æ‰‹æ®µï¼Œè¯¥BMIèŒƒå›´NIPTæ£€æµ‹éš¾åº¦æé«˜")

        print()

print("æ‰€æœ‰å›¾è¡¨å·²ä¿å­˜ä¸ºPNGæ–‡ä»¶ï¼Œæ–‡ä»¶åä»¥'Q3-'ä¸ºå‰ç¼€")
